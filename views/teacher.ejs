<!DOCTYPE html>
<html>
<head>
	  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css" integrity="sha384-9gVQ4dYFwwWSjIDZnLEWnxCjeSWFphJiwGPXr1jddIhOegiu1FwO5qRGvFXOdJZ4" crossorigin="anonymous">
	<title></title>
</head>
<body>
	<h1 align="center">Teacher Data</h1>
    <div style="text-align:center">
      <a href="/teachers/add">add teacher</a> || <a href="/">home</a>
    </div>
	<table class="table" border="1">
		<tr>
		<th>Id</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th>Options</th>
		</tr>
		<% teacher.forEach(teacher=>{ %>
		<tr>
			<th> <%= teacher.id %> </th>
			<th> <%= teacher.first_name %> </th>
			<th> <%= teacher.last_name %> </th>
			<th> <%= teacher.email %> </th>
			<th> <a href="/teachers/edit/<%=teacher.id%>">Edit</a> | <a href="/teachers/delete/<%=teacher.id%>" onclick="return confirm('Delete this teacher?')">Delete</a></th>
		</tr>
	<%})%>
	</table>
</body>
</html>